{
    "name": "fomvasss/laravel-url-aliases",
    "description": "Use url-aliases (path) in Laravel project",
    "type": "composer-package",
    "keywords": [
        "fomvasss", "laravel", "alias", "url", "uri", "path"
    ],
    "authors": [
        {
            "name": "Vasyl Fomin",
            "email": "fomvasss@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php" : ">=7.1.3",
        "illuminate/support": "5.6.*|5.7.*|5.8.*|^6.0",
        "illuminate/database": "5.6.*|5.7.*|5.8.*|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Fomvasss\\UrlAliases\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fomvasss\\UrlAliases\\ServiceProvider"
            ],
            "aliases": {
                "UrlAliasLocalization": "Fomvasss\\UrlAliases\\Facades\\UrlAliasLocalization",
                "UrlAlias": "Fomvasss\\UrlAliases\\Facades\\UrlAlias"
            }
        }
    }
}
